    (<= (legal white (move ?x1 ?y1 ?x2 ?y2))
        (true (control white))
        (true (cell ?x1 ?y1 wKnight))
        (data (add1x ?x1 ?x2))
        (data (add2y ?y1 ?y2))
        (not (cell ?x2 ?y2 bKnight))
    )

    (<= (legal black (move ?x1 ?y1 ?x2 ?y2))
        (true (control black))
        (true (cell ?x1 ?y1 bKnight))
        (data (add1x ?x1 ?x2))
        (data (add2y ?y1 ?y2))
        (not (cell ?x2 ?y2 wKnight))
