    (<= (legal white noop)
        (true (control black)))

    (<= (legal black noop)
        (true (control white)))

    (<= (next (control black))
        (true (control white)))

    (<= (next (control white))
        (true (control black)))

    (<= (next (cell ?x ?y hole))
        (true (cell ?x ?y hole)))

    (<= (next (cell ?x1 ?y1 hole))
        (does ?player (move ?x1 ?y1 ?x2 ?y2)))

    (<= (next (cell ?x2 ?y2 wKnight))
        (does white (move ?x1 ?y1 ?x2 ?y2))
        (not (cell ?x2 ?y2 hole)))

    (<= (next (cell ?x2 ?y2 bKnight))
        (does black (move ?x1 ?y1 ?x2 ?y2))
        (not (cell ?x2 ?y2 hole)))

    (<= (wAlive)
        (true (cell ?x ?y wKnight))
    )

    (<= (bAlive)
        (true ( cell ?x ?y bKnight ))
    )

    (<= (goal white 100)
        (not (bAlive))
    )

    (<= (goal black 100)
        (not ( wAlive))
    )

    (<= (goal black 0)
        (not (bAlive))
    )

    (<= (goal white 100)
        (not ( bAlive))
    )


    (<= terminal
        (wAlive))

    (<= terminal
        (bAlive))

